    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="invesafeEntities" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="BusinessLayer.invesafeEntities, BusinessLayer" Origin="Configuration" />
